{
  "name": "malvidcomponents",
  "displayName": "MalvidComponents",
  "description": "Generates folder and file structure for malvid components",
  "author": "Stephane Goeuriot",
  "publisher": "stephanegoeuriot",
  "version": "0.0.6",
  "license": "MIT",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.37.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:extension.newMalvidComponentWithConfig",
    "onCommand:extension.newMalvidComponent"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.newMalvidComponentWithConfig",
        "title": "New Malvid Component With Config"
      },
      {
        "command": "extension.newMalvidComponent",
        "title": "New Malvid Component"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.newMalvidComponentWithConfig",
          "group": "navigation"
        },
        {
          "command": "extension.newMalvidComponent",
          "group": "navigation"
        }
      ]
    },
    "snippets": [
      {
        "language": "nunjucks",
        "path": "./snippets/njk.snippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/js.snippets.json"
      },
      {
        "language": "scss",
        "path": "./snippets/scss.snippets.json"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.37.0",
    "eslint": "^5.13.0",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.2"
  },
  "dependencies": {
    "change-case": "^3.1.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^8.1.0",
    "gitignore-globs": "^0.1.1",
    "glob": "^7.1.4",
    "glob-gitignore": "^1.0.14"
  },
  "repository": "https://github.com/stphn/vscode-malvid-folder"
}
